.ppg_card_img {
  width: 690rpx;
  height: 370rpx;
  margin: 0 auto;
  z-index: 0;
}

.ppg_menber_cardId {
  position: absolute;
  right: 64rpx;
  top: 46rpx;
  border-radius: 10rpx;
  padding: 2rpx 10rpx;
  font-size: 32rpx;
  background-color: rgba(255, 255, 255, 0.50);
}

.menbername {
  font-size: 1.875rem;
  text-align: center;
  padding: 46rpx 0;
  margin: 0 auto;
}

.ppg_account_hd {
  background: #fff;
  height: 110rpx;
  box-sizing: border-box;
  box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.10);
}

.ppg_bounceInLeft {
  background: #fff;
  margin: 0;
