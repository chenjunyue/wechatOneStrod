  flex-wrap: nowrap;
  justify-content: space-around;
}

.ppg_bounceInLeft {
  /*text-align: center;*/
}

.ppg_mypromotion {
  text-align: center;
  width: 100%;
  position: relative;
}

.ppg_up_line::before {
  content: ' ';
  display: inline-block;
  height: 52rpx;
  width: 2rpx;
  border-right: 2rpx solid #e8e8e8;
  position: relative;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 0rpx;
}

.ppg_fontRed {
  margin-top: 18rpx;
  margin-bottom: 8rpx;
  font-size: 32rpx;
  color: #ff5556;
  font-weight: bold;
}

.ppg_fontGray {
  margin-top: 8rpx;
  margin-bottom: 14rpx;
  font-size: 26rpx;
  color: #3a3c4c;
}

.ppg_listBlock {
  margin-top: 10rpx;
  background: #fff;
  box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.10);
  box-sizing: border-box;
}

.ppg_itemContent {
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  box-sizing: border-box;
  position: relative;
}

