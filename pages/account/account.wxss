/* account.wxss */

.ppg_page {
  background-color: #f7f7f7;
  box-sizing: border-box;
}

.ppg_menber {
  background: #fff;
  height: 142rpx;
  box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.10);
  margin-top: 10rpx;
  position: relative;
}

.ppg_menber image {
  float: left;
  width: 110rpx;
  height: 110rpx;
  margin-left: 22rpx;
  margin-top: 16rpx;

}

.ppg_menber_name {
  font-size: 32rpx;
  color: #757575;
  line-height: 45rpx;
  margin-left: 152rpx;
  padding-top: 40rpx;
}

.ppg_menber_set {
  font-size: 26rpx;
  color: #3A3C4C;
  line-height: 37rpx;
  margin-left: 152rpx;
}

.ppg_mian {
  height: 356rpx;
  width: 100%;
  overflow: hidden;
}

/*签到页面的CSS*/

.ppg_sign {
  height: 342rpx;
  width: 100%;
  text-align: center;
  box-sizing: border-box;
  margin-top: 24rpx;
  position: relative;
}

.ppg_sign_badge {
  font-size: 24rpx;
  color: #fff;
  position: relative;
}

.ppg_sign_badge1 {
  height: 120rpx;
  width: 120rpx;
}

.ppg_sign_tag {
  background: url('data:image/png;base64,') no-repeat;
  background-size: 154rpx 52rpx;
  height: 52rpx;
  width: 154rpx;
  text-align: center;
  position: absolute;
  top: 90rpx;
  left: 50%;
  transform: translateX(-50%);
  line-height: 1.4;
}

.ppg_sign_total {
  font-size: 26rpx;
  color: #3a3c4c;
  margin-top: 32rpx;
}

.ppg_sign_list {
  display: flex;
  flex-wrap: nowrap;
  justify-content: space-around;
  margin-top: 40rpx;
}

.ppg_sign_item {
  font-size: 20rpx;
  color: #939393;
  line-height: 1.4;
  margin-top: 18rpx;
  width: 100%;
  position: relative;
}

.ppg_sign_item image {
  width: 52rpx;
  height: 52rpx;
}

.ppg_sign_item_hd {
  margin-bottom: 8rpx;
}

.ppg_hyphen {
  width: 80%;
  height: 4rpx;
  background-color: #bbb;
  position: absolute;
  top: 244rpx;
  right: 50%;
  transform: translateX(50%);
}

.ppg_advance {
  width: 80rpx;
  height: 4rpx;
  background-color: #ffde95;
}

.ppg_advance::before {
  content: ' ';
  display: inline-block;
  width: 13rpx;
  height: 13rpx;
  border-radius: 26rpx;
  background-color: #ffde95;
  float: right;
  position: relative;
  bottom: 6rpx;
}

.ppg_row {
