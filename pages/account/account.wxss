.ppg_itemContent:active {
  background-color: #ade1f5;
}

.ppg_line_dowm {
  border-bottom: #e5e5e5 1px solid;
}

.ppg_item_img {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 22rpx;
}

.ppg_red_dot {
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  background-color: red;
  border-radius: 12rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 226rpx;
}

.arrows::before {
  content: " ";
  display: inline-block;
  height: 16rpx;
  width: 16rpx;
  border-width: 2px 2px 0 0;
  border-color: #bbb;
  border-style: solid;
  position: absolute;
  top: 50%;
  transform: rotate(45deg) translateY(-50%);
  right: 38rpx;
}

.closeIoc {
  /*background-image: url('../../image/icon_logout@2x.png');*/
  background-image: url('../../image/icon_logout@2x.png');
  background-size: 34rpx 38rpx;
  padding-right: 70rpx;
  background-repeat: no-repeat;
  background-position: calc(100% - 30rpx) center;
}

.ppg_but {
  position: fixed;
  bottom: 0rpx;
  width: 100%;
  background-color: #e69a40;
  color: #fff;
}

